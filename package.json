{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Redux Koa Bookshelf Mocha SASS Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=$NODE_PATH:./shared node --harmony .",
    "dev": "npm run start & webpack-dev-server --progress --color",
    "build": "NODE_ENV=production webpack --progress --color -p --config webpack.prod.config.js",
    "test": "mocha --compilers js:babel-core/register --require ./tests/test_helper.js 'tests/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlsen/react-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlsen/react-boilerplate/issues"
  },
  "homepage": "https://github.com/mlsen/react-boilerplate#readme",
  "dependencies": {
    "css-loader": "^0.19.0",
    "history": "^1.12.5",
    "html-webpack-plugin": "^1.6.2",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.1.1",
    "koa": "^1.1.0",
    "koa-cors": "0.0.16",
    "koa-router": "^5.2.3",
    "lodash": "^3.10.1",
    "node-sass": "^3.3.3",
    "object-assign": "^4.0.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.2",
    "redux-thunk": "^1.0.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "chai-immutable": "^1.3.0",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "redux-logger": "^2.0.3",
    "webpack-dev-server": "^1.12.0"
  }
}
