{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Redux Koa Bookshelf Mocha SASS Boilerplate",
  "main": "index.js",
  "scripts": {
    "webserver": "node api/src/server.js",
    "test": "mocha --compilers js:babel-core/register --require ./app/tests/test_helper.js 'app/tests/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "build": "node_modules/.bin/webpack",
    "dev": "node_modules/.bin/webpack-dev-server",
    "lint": "eslint . --ext .js --ext .jsx",
    "deploy": "NODE_ENV=production webpack -p",
    "clean": "rm -rf app/dist app/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlsen/react-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlsen/react-boilerplate/issues"
  },
  "homepage": "https://github.com/mlsen/react-boilerplate#readme",
  "dependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "bookshelf": "^0.8.2",
    "css-loader": "^0.19.0",
    "html-webpack-plugin": "^1.6.2",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.1.1",
    "knex": "^0.8.6",
    "koa": "^1.1.0",
    "koa-cors": "0.0.16",
    "koa-router": "^5.2.3",
    "node-sass": "^3.3.3",
    "pg": "^4.4.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.2",
    "redux-thunk": "^1.0.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "chai": "^3.3.0",
    "chai-immutable": "^1.3.0",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "webpack-dev-server": "^1.12.0"
  }
}
